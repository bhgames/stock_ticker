{{input value=query }}

{{#if query}}
  <p>Displaying results for "{{query}}"</p>

  <ul>
    {{#each model as |s|}}
      <li><a {{action "draw" s}}>{{s.name}} - {{s.symbol}}</a></li>
    {{/each}}     
  </ul>
{{/if}}
<br />

{{sorry-message histories=currentHistory stock=currentStock}}
{{daily-graph histories=currentHistory stock=currentStock}}
{{outlet}}